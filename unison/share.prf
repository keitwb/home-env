root = /share
root = ssh://benkeith-server:2222//share

log = true

sshargs = -p 2222 
perms = 0
copyprog = rsync -e 'ssh -p 2222' --inplace --compress
copythreshold = 500
fastcheck = true

ignore = Path System Volume Information
ignore = Path RECYCLER
ignore = Path code/android/android-src
ignore = Path code/android/android-sdk*
